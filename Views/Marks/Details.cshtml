@model oop_CA.Models.Mark;
@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>
<div>
    <h4>Mark</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Student Id
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.studentId)
        </dd>
        <dt class="col-sm-2">
            Subject
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.subject)
        </dd>
        <dt class="col-sm-2">
            Teacher Comment
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.teacherComment)
        </dd>
        <dt class="col-sm-2">
            Coefficient
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.coefficient)
        </dd>
        <dt class="col-sm-2">
            Value
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.value)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.id">Edit</a> |
