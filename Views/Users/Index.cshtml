user index page

@if ((string)ViewData["Logged"] == "OK")
{
    <a asp-action="Logout">Logout</a>
    <a asp-action="ChangePassword">Change password</a>
    <a asp-action="Update">Update</a>
    @if ((string)ViewData["Admin"] == "OK")
    {
        <a asp-action="Manage">Manage</a>
    }
}
else
{
    <a asp-action="Login">Login</a>
    <a asp-action="Register">Register</a>
}
